function youtube() {
    var oiframes = document.getElementsByClassName('youtube-item');
    for (var i=0, n=oiframes.length|0; i < n; i=i+1|0) {
        var div = oiframes[i];
        if (div.tagName == 'DIV') {
            var div = oiframes[i];
            var iframe = document.createElement("iframe");
            iframe.setAttribute('class', div.getAttribute('class'));
            iframe.setAttribute('src', div.getAttribute('id'));
            div.parentNode.replaceChild(iframe, div);
        }
    }
    return false;
}
function timerex() {
    try {
	youtube();
    } catch(err) {
    }

    try {
	loadStream();
    } catch(err) {
    }

    try {
	loadAds();
    } catch(err) {
    }

    return false;
}
