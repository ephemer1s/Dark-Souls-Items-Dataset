        var mapA = $('#mapA');
        var mapB = $('#mapB');
        if (mapA != null && mapB != null) {
                $('#mapA').hide();
                $('#mapB').show();
        }
}
if (params != null && params.code != null && params.code == 'mapC') {
        var mapA = $('#mapA');
        var mapB = $('#mapB');
	var mapC = $('#mapC');
        if (mapA != null && mapB != null && mapC != null) {
                $('#mapA').hide();
                $('#mapC').show();
        }
}



});

		</script>
