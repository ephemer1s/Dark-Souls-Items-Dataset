function loadStream() {
        if (ppp && window.innerWidth < 1000) {
                var options = {width:340,height:198,channel:"fextralife",muted:true,theme:"dark",layout:"video"};
                var player = new Twitch.Embed("fextrastream", options);
//        } else if (ppp && window.matchMedia("(min-width: 1100px)").matches) {
//                if (ppp && window.matchMedia("(min-width: 1920px)").matches) {
//                        var options = {width:340,height:398,channel:"fextralife",muted:true,theme:"dark",layout:"video-with-chat"};
//                        var player = new Twitch.Embed("fextrastream", options);
//                } else {
//                        var options = {width:340,height:198,channel:"fextralife",muted:true,theme:"dark",layout:"video"};
//                        var player = new Twitch.Embed("fextrastream", options);
//                }
        }
	return false;
}
function loadAds() {
    if (ua!='100'&&ua!='110'&&ua!='210'&&ua!='111'&&ua!='211') {
        if (window.innerWidth >= 860) {
            var element = document.createElement("script");
            element.src = "https://s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5382e7e20b0e47b1";
            document.body.appendChild(element);
        } else {
            var menuAddA = document.getElementById('menu-add-a');
            if (menuAddA != null) {
                menuAddA.remove();
            }
            var menuAddB = document.getElementById('menu-add-b');
            if (menuAddB != null) {
                menuAddB.remove();
            }
	}
