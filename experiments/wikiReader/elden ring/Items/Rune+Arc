//                        var player = new Twitch.Embed("fextrastream", options);
//                }
        }
	return false;
}
function loadAds() {
    if (ua!='100'&&ua!='110'&&ua!='210'&&ua!='111'&&ua!='211') {
        if (window.innerWidth >= 860) {
            var element = document.createElement("script");
            element.src = "https://s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5382e7e20b0e47b1";
            document.body.appendChild(element);
        } else {
            var menuAddA = document.getElementById('menu-add-a');
            if (menuAddA != null) {
                menuAddA.remove();
            }
            var menuAddB = document.getElementById('menu-add-b');
            if (menuAddB != null) {
                menuAddB.remove();
            }
	}
	!function(e){var s=new XMLHttpRequest;s.open("GET","https://api.enthusiastgaming.net/scripts/cdn.enthusiast.gg/script/eg-aps/release/eg-aps-bootstrap-v2.0.0.bundle.js?site=fextralife.com",!0),s.onreadystatechange=function(){var t;4==s.readyState&&(200<=s.status&&s.status<300||304==s.status)&&((t=e.createElement("script")).type="text/javascript",t.text=s.responseText,e.head.appendChild(t))},s.send(null)}(document);
	(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start': new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer','GTM-KW2R3PT');
    }
    return false;
}
function youtube() {
    var oiframes = document.getElementsByClassName('youtube-item');
    for (var i=0, n=oiframes.length|0; i < n; i=i+1|0) {
        var div = oiframes[i];
        if (div.tagName == 'DIV') {
            var div = oiframes[i];
            var iframe = document.createElement("iframe");
            iframe.setAttribute('class', div.getAttribute('class'));
            iframe.setAttribute('src', div.getAttribute('id'));
            div.parentNode.replaceChild(iframe, div);
        }
    }
    return false;
}
function timerex() {
    try {
	youtube();
    } catch(err) {
    }

    try {
	loadStream();
    } catch(err) {
    }

    try {
	loadAds();
    } catch(err) {
    }

    return false;
}
document.addEventListener('DOMContentLoaded', (event) => {
    setTimeout(timerex, 3000);
    try {
        if (window.innerWidth >= 680) {
            $('.sliderload').load('//swcdn.fextralife.com/file/slider.html?v='+new Date().getTime(), function() {
                $('img.lazyload').lazyload({failure_limit : 999999});
            });
        }
    } catch(err) {
    }


var params = {};
var ximapa = document.getElementById('ximap0');
var ximapb = document.getElementById('ximap1');
var ximapc = document.getElementById('ximap2');
if (ximapa != null || ximapb != null || ximapc != null) {
    window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m, key, value) {
        params[key] = value;
    });
}

if (ximapa != null) {
    if (params != null && params.id != null) {
        ximapa.src = ximapa.src + '?id=' + params.id + '&lat=' + params.lat + '&lng=' + params.lng + '&zoom=' + params.zoom;
    }
}
if (ximapb != null) {
    if (params != null && params.id != null) {
        ximapb.src = ximapb.src + '?id=' + params.id + '&lat=' + params.lat + '&lng=' + params.lng + '&zoom=' + params.zoom;
    }
}
if (ximapc != null) {
    if (params != null && params.id != null) {
        ximapc.src = ximapc.src + '?id=' + params.id + '&lat=' + params.lat + '&lng=' + params.lng + '&zoom=' + params.zoom;
    }
}
if (params != null && params.code != null && params.code == 'mapB') {
        var mapA = $('#mapA');
        var mapB = $('#mapB');
        if (mapA != null && mapB != null) {
                $('#mapA').hide();
                $('#mapB').show();
        }
}
if (params != null && params.code != null && params.code == 'mapC') {
        var mapA = $('#mapA');
        var mapB = $('#mapB');
	var mapC = $('#mapC');
        if (mapA != null && mapB != null && mapC != null) {
                $('#mapA').hide();
                $('#mapC').show();
        }
}



});

		</script>
		<script data-cfasync="false" type="text/javascript">
			function loadJSON(callback) { var xobj = new XMLHttpRequest();xobj.overrideMimeType("application/json");xobj.open('GET', '/ws/user/unread-msgs', true);xobj.onreadystatechange = function () {if (xobj.readyState == 4 && xobj.status == "200") {callback(xobj.responseText);}};xobj.send(null);  }if (c != null) {loadJSON(function(response) {if (parseInt(response) > 0) {var nmsgs = response;var finbox = $('.finbox');finbox.text('Inbox (' + nmsgs + ')');var fuavtr = $('#user-avatar');fuavtr.replaceWith('<span id="priv-msgs" style="display: inline-block;width: 24px;height: 24px;line-height: 24px;background-color: #a00;color: #fff;font-weight: 700;text-align: center;vertical-align: middle;border-radius: 50%;margin-right: 3px;">' + nmsgs + '</span><img id="avatar" src="' + fuavtr.attr('src') + '" alt="" style="border-left-width:0px;width:30px;height:30px;"/>');}});}
		</script>
