<h3 class="bonfire">Kris Blade Moveset<strong><br></strong></h3> 
<div class="row"> 
 <div class="col-sm-4"> 
  <div class="table-responsive"> 
   <table class="wiki_table"> 
    <thead> 
     <tr> 
      <th>Attack Type</th> 
      <th>Effect</th> 
     </tr> 
    </thead> 
    <tbody> 
     <tr> 
      <td>One-handed R1</td> 
      <td>Horizontal cuts</td> 
     </tr> 
     <tr> 
      <td>One-handed R2</td> 
      <td>Thrust plus additional vertical cut with second input</td> 
     </tr> 
     <tr> 
      <td>Two-handed R1</td> 
      <td>Vertical cuts</td> 
     </tr> 
     <tr> 
      <td>Two-handed R2</td> 
      <td>Horizontal cut plus second horizontal cut with second input</td> 
     </tr> 
     <tr> 
      <td>Running/Backstep attack</td> 
      <td>Horizontal cut</td> 
     </tr> 
     <tr> 
      <td>Rolling attack</td> 
      <td>Vertical cut</td> 
     </tr> 
     <tr> 
      <td>Push</td> 
      <td>Quick push</td> 
     </tr> 
     <tr> 
      <td>One-handed L1</td> 
      <td>Block</td> 
     </tr> 
     <tr> 
      <td>One-handed L2</td> 
      <td>Horizontal cut</td> 
     </tr> 
    </tbody> 
   </table> 
